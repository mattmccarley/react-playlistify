    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.4.9",
    "concurrently": "^4.1.0",
    "postcss-cli": "^6.1.2",
    "tailwindcss": "^0.7.4"
  },
  "proxy": "http://localhost:9000/"
}
