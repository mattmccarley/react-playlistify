{
  "name": "react-playlistify",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.0",
    "lodash": "^4.17.11",
    "netlify-lambda": "^1.4.9",
    "querystring": "^0.2.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.5",
    "reactn": "^2.0.1",
    "request": "^2.88.0",
    "serverless-http": "^2.0.2",
    "spotify-web-api-js": "^1.2.0"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
